// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void AgeCheck() {
    int age;
    cout << "What is your age:\n";
    cin >> age;
    if (age >= 16) {
        cout <<"Adult\n";
    } else {
        cout << "Minor";
    }
}

int main() {
    AgeCheck();
    return 0;
}